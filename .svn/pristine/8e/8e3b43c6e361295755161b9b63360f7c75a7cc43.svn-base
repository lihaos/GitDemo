<script type="text/javascript">

 function shanChu(id){
	 
	 if(confirm("你确定要删除吗？")==true){
	   $.getJSON("/noteDelete?id="+id,null,function(pd){
		   if(pd==1){
			   $("#rem"+id).remove();
			   alert("删除成功");
		   }
		   
	   })	 
		 
	 }
	 
 }


</script>
